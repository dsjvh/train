<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.spring_test.mapper.EmployeeMapper">


    <select id="selectByName" parameterType="java.lang.String" resultType="com.example.spring_test.entity.Employee">
        SELECT * FROM tbl_employee WHERE last_name like concat('%',#{name},'%')
    </select>
</mapper>